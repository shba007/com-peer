{
  "name": "untest",
  "description": "Learn by quick question answer competition with peers",
  "author": "Shirsendu Bairagi <shirsendu2001@gmail.com>",
  "version": "0.2.6",
  "private": true,
  "type": "module",
  "engines": {
    "node": "^20.15.0"
  },
  "license": "MIT",
  "scripts": {
    "postinstall": "nuxt prepare && prisma generate",
    "dev": "nuxt dev",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier .  --write",
    "build": "nuxt build",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "start": "node .output/server/index.mjs",
    "tunnel": "npx untun tunnel --port 3000  http://localhost:3000",
    "clean": "nuxt cleanup",
    "db:seed": "prisma db seed",
    "db:backup": "npx tsx prisma/backup.ts",
    "db:pull": "prisma db pull",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:deploy": "prisma migrate deploy",
    "db:reset": "prisma migrate reset",
    "prisma:studio": "prisma studio",
    "docker:build": "docker build --build-arg VERSION_TAG=v0.2.6 -t untest:v0.2.6 .",
    "docker:start": "docker run --detach --name untest --env-file .env.prod -p 1000:3000 untest:v0.2.6"
  },
  "prisma": {
    "seed": "npx tsx prisma/seed.ts"
  },
  "dependencies": {
    "canvas-confetti": "^1.9.3",
    "jsonwebtoken": "^9.0.2",
    "prisma": "^5.17.0",
    "yaml": "^2.4.5"
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.7",
    "@nuxt/eslint": "^0.3.13",
    "@nuxt/fonts": "^0.7.1",
    "@nuxt/image": "^1.7.0",
    "@nuxt/test-utils": "^3.13.1",
    "@nuxtjs/color-mode": "^3.4.2",
    "@nuxtjs/seo": "^2.0.0-rc.16",
    "@nuxtjs/tailwindcss": "^6.12.1",
    "@pinia/nuxt": "^0.5.1",
    "@prisma/client": "^5.17.0",
    "@types/canvas-confetti": "^1.6.4",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^20.14.11",
    "@vite-pwa/nuxt": "^0.9.1",
    "@vueuse/nuxt": "^10.11.0",
    "eslint-config-prettier": "^9.1.0",
    "nuxt": "^3.12.4",
    "nuxt-gtag": "^2.0.7",
    "nuxt-icons": "^3.2.1",
    "prettier": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.3",
    "vue": "latest"
  }
}
